@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title> Add Technician </title>
</head>
<body>
    <form action="/my-handling-form-page" method="post">
        <div>
            <div>
                <label for="name">Name:</label>
